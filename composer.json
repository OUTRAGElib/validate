{
    "name": "outragelib/validate",
    "description": "Validation library for OUTRAGElib",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "~6.0",
        "outragelib/delegator": "^1.0",
        "outragelib/fs-temp": "^1.0",
        "outragelib/objectlist": "^1.0",
        "outragelib/psr7-file-stream": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "David Weston",
            "email": "westie@typefish.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "OUTRAGElib\\Validate\\": "./lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OUTRAGElib\\Validate\\Tests\\": "./tests"
        }
    },
    "suggest": {
        "symfony/validator": "^3.3",
        "zendframework/zend-validator": "^2.10"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "illuminate/validation": "^5.4",
        "phpstan/phpstan": "^0.9.0@dev",
        "phpunit/phpunit": "^6.4@dev",
        "symfony/validator": "^3.3",
        "theseer/phpdox": "dev-master",
        "zendframework/zend-validator": "^2.10"
    }
}